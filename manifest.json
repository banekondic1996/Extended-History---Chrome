{
  "manifest_version": 3,
  "name": "Extended History",
  "version": "3.0.0",
  "description": "Replaces Chrome history. Unlimited retention, sessions, time tracking, bookmarks, multi-device.",
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "browsingData",
    "contextMenus",
    "favicon",
    "history",
    "scripting",
    "sessions",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extended History"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
